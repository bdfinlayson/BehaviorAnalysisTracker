<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <base href="/" />
    <title>Behavior Analysis Tracker</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="viewport" content="user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimal-ui" />
    <meta name="apple-mobile-web-app-status-bar-style" content="yes" />
    <link rel="shortcut icon" href="/baticon.png" type="image/x-icon" />
    <link rel="stylesheet" href="~/Content/mobile-angular-ui-hover.css" />
    <link rel="stylesheet" href="~/Content/mobile-angular-ui-base.css" />
    <link rel="stylesheet" href="~/Content/mobile-angular-ui-desktop.css" />
    <link rel="stylesheet" href="~/Content/custom.css" />
    <link rel="stylesheet" href="~/Content/session.css" />
    @Scripts.Render("~/bundles/modernizr")
</head>
<body ng-app="batApp" ng-controller="MainController">
    <div ng-include="'sidebar.html'" ui-track-as-search-param='true' class="sidebar sidebar-left"></div>

    <div class="app">

        <div class="navbar navbar-app navbar-absolute-top">
            <div class="navbar-brand navbar-brand-center" ui-yield-to="title">
                B.A.T.
            </div>
            <div class="btn-group pull-left">
                <div ui-toggle="uiSidebarLeft" class="btn sidebar-toggle">
                    <i class="fa fa-list"></i> Menu
                </div>
            </div>
        </div>

        <div class="app-body" ng-class="{loading: loading}">
            <div ng-show="loading" class="app-content-loading">
                <i class="fa fa-circle-o-notch fa-spin loading-spinner"></i>
            </div>
            <div class="app-content">
                <ng-view>

                </ng-view>
            </div>
        </div>

    </div>

    <div ui-yield-to="modals"></div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script src="~/Scripts/js/angular.min.js"></script>
    <script src="~/Scripts/js/angular-route.min.js"></script>
    <script src="~/Scripts/js/mobile-angular-ui.min.js"></script>
    <script src="~/Scripts/js/jquery.min.js"></script>
    <script src="~/Scripts/js/underscore-min.js"></script>
    <script src="~/Scripts/js/moment.js"></script>
    <script src="~/Scripts/App/main.js"></script>
    <script src="~/Scripts/App/main.config.js"></script>
    <script src="~/Scripts/App/main.constants.js"></script>
    <script src="~/Scripts/App/main.controller.js"></script>
    <script src="~/Scripts/App/auth/auth.controllers.js"></script>
    <script src="~/Scripts/App/auth/auth.factory.js"></script>
    <script src="~/Scripts/App/auth/auth.routes.js"></script>
    <script src="~/Scripts/App/dashboard/dashboard.routes.js"></script>
    <script src="~/Scripts/App/dashboard/datacollector.factory.js"></script>
    <script src="~/Scripts/App/dashboard/datacollector.controller.js"></script>
    <script src="~/Scripts/App/dashboard/codesets.factory.js"></script>
    <script src="~/Scripts/App/dashboard/codesets.controller.js"></script>
    <script src="~/Scripts/App/dashboard/sessiondata.factory.js"></script>
    <script src="~/Scripts/App/dashboard/sessiondata.controller.js"></script>
    <script src="~/Scripts/App/dashboard/analyzedata.controller.js"></script>
</body>
</html>



